<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>
        <UnoVersion>4.10.22</UnoVersion>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageVersion Include="Microsoft.Extensions.Logging.Console" Version="7.0.0" IsImplicitlyDefined="true" />
        <PackageVersion Include="Microsoft.Windows.Compatibility" Version="7.0.0" IsImplicitlyDefined="true" />
        <PackageVersion Include="Microsoft.Windows.SDK.BuildTools" Version="10.0.22621.756" IsImplicitlyDefined="true" />
        <PackageVersion Include="Microsoft.WindowsAppSDK" Version="1.3.230602002" IsImplicitlyDefined="true" />
        <PackageVersion Include="SkiaSharp.Skottie" Version="2.88.3" IsImplicitlyDefined="true" />
        <PackageVersion Include="SkiaSharp.Views.Uno.WinUI" Version="2.88.3" IsImplicitlyDefined="true" />
        <PackageVersion Include="Swashbuckle.AspNetCore" Version="6.5.0" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.Core.Extensions.Logging.Singleton" Version="4.0.1" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.Extensions.Logging.OSLog" Version="1.4.0" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.Extensions.Logging.WebAssembly.Console" Version="1.4.0" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.Resizetizer" Version="1.1.0" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.UniversalImageLoader" Version="1.9.36" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.Wasm.Bootstrap" Version="7.0.23" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.Wasm.Bootstrap.DevServer" Version="7.0.23" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.Wasm.Bootstrap.Server" Version="7.0.23" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.WinUI" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.WinUI.RemoteControl" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.WinUI.Skia.Gtk" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.WinUI.WebAssembly" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageVersion Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageVersion Include="Xamarin.Google.Android.Material" Version="1.8.0" IsImplicitlyDefined="true" />
    </ItemGroup>

    <ItemGroup Condition="'$(UseUnoMobile)' == 'true'">
        <PackageReference Include="Uno.WinUI" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="7.0.0" IsImplicitlyDefined="true" />
        <PackageReference Include="Uno.Extensions.Logging.OSLog" Version="1.6.0" IsImplicitlyDefined="true" />
        <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageReference Include="Uno.WinUI.RemoteControl" Condition="'$(Configuration)'=='Debug'" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
    </ItemGroup>

    <Choose>
        <When Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'android'">
            <ItemGroup>
                <PackageReference Include="Xamarin.Google.Android.Material" Version="1.9.0.3" IsImplicitlyDefined="true" />
                <PackageReference Include="Uno.UniversalImageLoader" Version="1.9.36" IsImplicitlyDefined="true" />
            </ItemGroup>
        </When>
        <When Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'ios'">
        </When>
        <When Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'maccatalyst'">
        </When>
    </Choose>

    <ItemGroup Condition="'$(UseUnoSkiaGtk)' == 'true'">
        <PackageReference Include="Uno.WinUI.Skia.Gtk" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
        <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="7.0.0" IsImplicitlyDefined="true" />
        <PackageReference Include="SkiaSharp.Views.Uno.WinUI" Version="2.88.6" IsImplicitlyDefined="true" />
        <PackageReference Include="SkiaSharp.Skottie" Version="2.88.6" IsImplicitlyDefined="true" />
        <PackageReference Include="Uno.WinUI.RemoteControl" Version="$(UnoVersion)" IsImplicitlyDefined="true" Condition="'$(Configuration)'=='Debug'" />
        <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="$(UnoVersion)" IsImplicitlyDefined="true" />
    </ItemGroup>

    <ItemGroup Condition="'$(UseUnoWebAssembly)' == 'true'">
    </ItemGroup>

    <ItemGroup Condition="'$(UseUnoWindows)' == 'true'">
    </ItemGroup>
    
</Project>
