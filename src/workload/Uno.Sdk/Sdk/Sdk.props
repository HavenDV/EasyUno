<Project>

    <!--
      Used only if Sdk is specified via `<Project Sdk="Uno.Sdk">`
    -->
    
    <Import Project="Sdk.props" Sdk="Microsoft.NET.Sdk.Web" Condition="'$(TargetFramework)' == '' OR '$(TargetFramework)' == 'net7.0' OR '$(TargetFramework)' == 'net8.0' OR '$(TargetFramework)' == 'net7.0-webassembly' OR '$(TargetFramework)' == 'net8.0-webassembly'"/>
    <Import Project="Sdk.props" Sdk="Microsoft.NET.Sdk" Condition="'$(TargetFramework)' != '' AND '$(TargetFramework)' != 'net7.0' AND '$(TargetFramework)' != 'net8.0' AND '$(TargetFramework)' != 'net7.0-webassembly' AND '$(TargetFramework)' != 'net8.0-webassembly'"/>

    <PropertyGroup>
        <UseUno>true</UseUno>
        <UseUnoSdkWay>true</UseUnoSdkWay>
    </PropertyGroup>
    
</Project>