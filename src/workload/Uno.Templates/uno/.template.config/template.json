{
  "$schema": "http://json.schemastore.org/template",
  "author": "HavenDV",
  "classifications": [ "Uno" ],
  "identity": "Uno.UnoApp",
  "name": "Uno .NET Application",
  "description": "A project for creating an Uno .NET application",
  "shortName": "uno",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "UnoApp1",
  "preferNameDirectory": true,
  "primaryOutputs": [
    { "path": "UnoApp1.csproj" }
  ],
  "symbols": {
    "packageName": {
      "type": "parameter",
      "description": "Overrides the package name in the uno-manifest.xml",
      "datatype": "string",
      "replaces": "com.companyname.UnoApp1"
    }
  },
  "defaultName": "UnoApp1"
}